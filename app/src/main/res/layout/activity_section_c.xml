<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Section_C_Activity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#D3D3D3">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="16dp"
            android:background="@drawable/rounded_background">

            <!-- 뒤로가기 -->
            <ImageView
                android:id="@+id/back_arrow"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/ic_back"
                android:layout_alignParentStart="true"
                app:tint="#FFFFFF" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="C구역"
                android:textColor="#FFFFFF"
                android:fontFamily="@font/pretendard_bold"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:textSize="20sp"/>


            <ImageView
                android:id="@+id/reload"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_alignParentEnd="true"
                android:src="@drawable/ic_reload"
                app:tint="#FFFFFF" />


        </RelativeLayout>



        <FrameLayout
            android:id="@+id/parkingline_1"
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:layout_marginTop="60dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/border_rectangle">

            <!-- 차 이미지 -->
            <ImageView
                android:id="@+id/car_1"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/b_empty_car_img"
                android:rotation="90"
                android:layout_gravity="center" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/parkingline_2"
            android:layout_width="90dp"
            android:layout_height="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/parkingline_1"
            android:background="@drawable/border_rectangle">

            <!-- 차 이미지 -->
            <ImageView
                android:id="@+id/car_2"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/b_empty_car_img"
                android:rotation="90"
                android:layout_gravity="center" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/parkingline_3"
            android:layout_width="90dp"
            android:layout_height="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/border_rectangle">

            <!-- 차 이미지 -->
            <ImageView
                android:id="@+id/car_3"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/b_empty_car_img"
                android:rotation="90"
                android:layout_gravity="center" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/parkingline_4"
            android:layout_width="90dp"
            android:layout_height="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/parkingline_1"
            android:background="@drawable/border_rectangle">

            <!-- 차 이미지 -->
            <ImageView
                android:id="@+id/car_4"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/b_empty_car_img"
                android:rotation="90"
                android:layout_gravity="center" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/parkingline_5"
            android:layout_width="90dp"
            android:layout_height="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/border_rectangle">

            <!-- 차 이미지 -->
            <ImageView
                android:id="@+id/car_5"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/b_empty_car_img"
                android:rotation="90"
                android:layout_gravity="center" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/parkingline_6"
            android:layout_width="90dp"
            android:layout_height="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/parkingline_1"
            android:background="@drawable/border_rectangle">

            <!-- 차 이미지 -->
            <ImageView
                android:id="@+id/car_6"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/b_empty_car_img"
                android:rotation="90"
                android:layout_gravity="center" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/parkingline_7"
            android:layout_width="90dp"
            android:layout_height="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/border_rectangle">

            <!-- 차 이미지 -->
            <ImageView
                android:id="@+id/car_7"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/b_empty_car_img"
                android:rotation="90"
                android:layout_gravity="center" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/parkingline_8"
            android:layout_width="90dp"
            android:layout_height="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/parkingline_1"
            android:background="@drawable/border_rectangle">

            <!-- 차 이미지 -->
            <ImageView
                android:id="@+id/car_8"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/b_empty_car_img"
                android:rotation="90"
                android:layout_gravity="center" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/parkingline_9"
            android:layout_width="90dp"
            android:layout_height="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/border_rectangle">
            <!-- 차 이미지 -->
            <ImageView
                android:id="@+id/car_9"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/b_empty_car_img"
                android:rotation="90"
                android:layout_gravity="center" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/parkingline_10"
            android:layout_width="90dp"
            android:layout_height="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/parkingline_1"
            android:background="@drawable/border_rectangle">

            <!-- 차 이미지 -->
            <ImageView
                android:id="@+id/car_10"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/b_empty_car_img"
                android:rotation="90"
                android:layout_gravity="center" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/parkingline_11"
            android:layout_width="90dp"
            android:layout_height="50dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/parkingline_1"
            android:background="@drawable/border_rectangle">

            <!-- 차 이미지 -->
            <ImageView
                android:id="@+id/car_11"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/b_empty_car_img"
                android:rotation="90"
                android:layout_gravity="center" />
        </FrameLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="16dp"
        android:background="#FFFFFF"
        android:gravity="center_horizontal"
        app:layout_constraintBottom_toBottomOf="parent">

        <TextView
            android:id="@+id/txt_normal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="일반 : 0 / 0"
            android:textSize="15sp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:text="●"
            android:textColor="#fa5252"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="사용 불가"
            android:textSize="15sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="●"
            android:textColor="#4cfa4f"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:text="사용 가능"
            android:textSize="15sp" />

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>